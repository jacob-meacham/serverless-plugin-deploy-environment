{
  "name": "serverless-plugin-deploy-environment",
  "version": "2.0.0",
  "description": "Plugin the augments the serverless provider environment with deployment variables",
  "main": "lib/index.js",
  "files": [
    "lib",
    "src"
  ],
  "engines": {
    "node": ">=12.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/DopplerLabs/serverless-plugin-deploy-environment.git"
  },
  "keywords": [
    "aws",
    "aws lambda",
    "amazon",
    "amazon web services",
    "serverless",
    "dopplerlabs.com"
  ],
  "author": "Matt Sills",
  "contributors": [
    {
      "name": "Jacob Meacham",
      "url": "http://jemonjam.com/"
    }
  ],
  "license": "MIT",
  "private": false,
  "bugs": {
    "url": "https://github.com/DopplerLabs/serverless-plugin-deploy-environment/issues"
  },
  "homepage": "https://github.com/DopplerLabs/serverless-plugin-deploy-environment",
  "scripts": {
    "lint": "eslint .",
    "eslint": "eslint",
    "test": "nyc ava --verbose",
    "test:watch": "ava --watch",
    "build:node": "cross-env BABEL_ENV=production babel src --out-dir lib",
    "build": "npm run lint && npm run test && npm run build:node",
    "docs": "esdoc .",
    "docs:deploy": "npm run docs && gh-pages -d doc",
    "ci:coverage": "nyc report --reporter=text-lcov | coveralls"
  },
  "dependencies": {
    "credstash": "1.0.44",
    "deasync-promise": "1.0.1",
    "lodash": "4.17.21",
    "winston": "2.3.1"
  },
  "devDependencies": {
    "@ava/babel": "1.0.1",
    "@babel/cli": "7.12.1",
    "@babel/core": "7.12.3",
    "@babel/preset-env": "7.12.1",
    "@babel/register": "7.12.1",
    "ava": "3.13.0",
    "coveralls": "3.1.0",
    "cross-env": "7.0.2",
    "esdoc": "^1.1.0",
    "esdoc-standard-plugin": "1.0.0",
    "eslint": "7.12.1",
    "eslint-config-standard": "16.0.1",
    "eslint-plugin-ava": "11.0.0",
    "eslint-plugin-import": "2.22.1",
    "eslint-plugin-node": "11.1.0",
    "eslint-plugin-promise": "4.2.1",
    "eslint-plugin-standard": "4.0.2",
    "gh-pages": "3.1.0",
    "nyc": "15.1.0",
    "rimraf": "3.0.2",
    "serverless": "2.9.0",
    "sinon": "9.2.1",
    "sinon-as-promised": "4.0.3"
  },
  "peerDependencies": {
    "serverless": ">=1.16.0"
  },
  "ava": {
    "require": [
      "@babel/register"
    ]
  },
  "babel": {
    "presets": [
      [
        "@babel/preset-env",
        {
          "targets": {
            "node": 12
          }
        }
      ]
    ]
  }
}
